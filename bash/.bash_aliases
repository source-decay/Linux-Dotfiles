# prompt is deleting more than 3 files
alias rm='rm -I --preserve-root'

# jumping around dirs
alias doc='cd ~/Documents'
alias dld='cd ~/Downloads'
alias dot='cd ~/dotfiles'
alias dev='cd ~/Documents/Development'

# Ricing information
alias rice='~/mybins/sysif.sh'
