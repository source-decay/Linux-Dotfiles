/* Rofi */
rofi.font:						Roboto 12
rofi.terminal:				xfce4-terminal
rofi.columns:					1
rofi.opacity:					98
rofi.bw:							0
rofi.sidebar-mode:		false
rofi.separator-style:	solid
rofi.hide-scrollbar:	true
rofi.color-enabled:		true
rofi.line-margin:			2
rofi.lines:						5
rofi.fixed-num-lines:	5
rofi.width:						600
rofi.m:								Acer 23
rofi.modi:						run
rofi.tokenize:				true
rofi.matching:				normal
rofi.click-to-exit:		true

rofi.color-window: #565575, argb:0002b36, argb:0000000
rofi.color-normal: #565575, #ffffff, argb:0eee8d5, #65b2ff, #ffffff
rofi.color-active: argb:065b2ff, #ffffff, argb:0eee8d5, argb:065b2ff, #ffffff
rofi.color-urgent: argb:0fdf6e3, #ff5458, argb:0eee8d5, argb:0dc322f, argb:0fdf6e3
